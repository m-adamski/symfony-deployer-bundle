{
    "name": "m-adamski/symfony-deployer-bundle",
    "description": "Bundle for Symfony for easier Deployer configuration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.1|^5.0",
        "symfony/framework-bundle": "^4.1|^5.0",
        "symfony/process": "^4.1|^5.0"
    },
    "require-dev": {
        "kint-php/kint": "^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\DeployerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adamski\\Symfony\\DeployerBundleTests\\": "tests/"
        }
    },
    "bin": [
        "bin/deployer",
        "bin/create-config"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
